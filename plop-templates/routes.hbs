module.exports = (routes, controller, autenticacaoMiddleware) => {
    routes.get('/{{lowerCase name}}', autenticacaoMiddleware, controller.findAll);
    routes.get('/{{lowerCase name}}/:id', autenticacaoMiddleware, controller.findOne);
    routes.post('/{{lowerCase name}}', autenticacaoMiddleware, controller.create);
    routes.put('/{{lowerCase name}}/:id', autenticacaoMiddleware, controller.update);
    routes.delete('/{{lowerCase name}}/:id', autenticacaoMiddleware, controller.delete);
}